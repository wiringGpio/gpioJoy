<?xml version="1.0" encoding="utf-8"?><GpioConfig>  <MCPs>    <MCP>      <Type>Mcp23017</Type>      <!-- chip at 0x23-->      <Address>35</Address>      <Base>200</Base>    </MCP>    <MCP>      <Type>Mcp23008</Type>      <!-- chip at 0x24-->      <Address>36</Address>      <Base>300</Base>    </MCP>    <MCP>      <Type>Mcp23008</Type>      <!--  chip at 0x27 -->      <Address>39</Address>      <Base>400</Base>    </MCP>	    <MCP>      <Type>Mcp23017</Type>      <!-- chip at 0x22-->      <Address>34</Address>      <Base>700</Base>    </MCP>  </MCPs>    <PCAs>    <PCA>      <!-- adafruit motor hat at default address 0x60-->      <Address>96</Address>      <Base>600</Base>      <Frequency>50</Frequency>    </PCA>  </PCAs>  <PinModes>	<!--  Motor Hat Pins -->    <Pin>      <Number>605</Number>      <Mode>Output</Mode>    </Pin>    <Pin>      <Number>606</Number>      <Mode>Output</Mode>    </Pin>    <Pin>      <Number>607</Number>      <Mode>PWMOutput</Mode>    </Pin>	<!--  Motor on MCP expansion pins -->    <Pin>      <Name>Motor One</Name>      <Number>405</Number>      <Mode>PWMOutput</Mode>      <Joystick scale="0.8">LeftStickUp</Joystick>    </Pin>    <Pin>      <Name>Motor One</Name>      <Number>405</Number>      <Mode>PWMOutput</Mode>      <Joystick scale="0.8">LeftTrigger</Joystick>    </Pin>		<!--  Seven segment display -->    <!-- Digits -->    <Pin>      <!-- White -->      <Number>700</Number>      <Mode>Output</Mode>    </Pin>    <Pin>      <!-- Grey -->      <Number>701</Number>      <Mode>Output</Mode>    </Pin>    <Pin>      <!-- Black  -->      <Number>702</Number>      <Mode>Output</Mode>    </Pin>    <!-- Segments a - g and decimal -->    <Pin>      <!-- a Purple/Silver -->      <Number>713</Number>      <Mode>Output</Mode>    </Pin>    <Pin>      <!-- b Yellow/Silver -->      <Number>715</Number>      <Mode>Output</Mode>    </Pin>    <Pin>      <!-- c Orange -->      <Number>711</Number>      <Mode>Output</Mode>    </Pin>    <Pin>      <!-- d Blue -->      <Number>709</Number>      <Mode>Output</Mode>    </Pin>    <Pin>      <!-- e Purple -->      <Number>708</Number>      <Mode>Output</Mode>    </Pin>    <Pin>      <!-- f Black/Silver-->      <Number>714</Number>      <Mode>Output</Mode>    </Pin>    <Pin>      <!-- g Brown -->      <Number>712</Number>      <Mode>Output</Mode>    </Pin>    <Pin>      <!-- dot Yellow -->      <Number>710</Number>      <Mode>Output</Mode>    </Pin>    <!-- Controller Model Pins -->	<!--  Left Side -->    <Pin>      <Name>Motor One</Name>      <Number>207</Number>      <Mode>PWMOutput</Mode>      <Joystick>LeftTrigger</Joystick>    </Pin>    <Pin>      <Name>NXT -</Name>      <Number>206</Number>      <Mode>Output</Mode>      <Joystick>LeftBumper</Joystick>    </Pin>    <Pin>      <Name>Motor One</Name>      <Number>205</Number>      <Mode>PWMOutput</Mode>      <Joystick>LeftStickUp</Joystick>    </Pin>    <Pin>      <Name>Motor One</Name>      <Number>203</Number>      <Mode>PWMOutput</Mode>      <Joystick>LeftStickDown</Joystick>    </Pin>    <Pin>      <Name>Motor +</Name>      <Number>200</Number>      <Mode>Output</Mode>      <Joystick>DpadUp</Joystick>    </Pin>    <Pin>      <Name>Motor x</Name>      <Number>208</Number>      <Mode>Output</Mode>      <Joystick>DpadRight</Joystick>    </Pin>    <Pin>      <Name>Motor -</Name>      <Number>209</Number>      <Mode>Output</Mode>      <Joystick>DpadDown</Joystick>    </Pin>    <Pin>      <Name>NXT x</Name>      <Number>210</Number>      <Mode>Output</Mode>      <Joystick>DpadLeft</Joystick>    </Pin>    <!-- Home Row -->    <Pin>      <Name>Back</Name>      <Number>211</Number>      <Mode>Output</Mode>      <Joystick>BackBtn</Joystick>    </Pin>    <Pin>      <Name>Home</Name>      <Number>212</Number>      <Mode>Output</Mode>      <Joystick>HomeBtn</Joystick>    </Pin>    <Pin>      <Name>Start</Name>      <Number>213</Number>      <Mode>Output</Mode>      <Joystick>StartBtn</Joystick>    </Pin>    <!-- Right Side -->    <Pin>      <Name>Motor Two</Name>      <Number>214</Number>      <Mode>PWMOutput</Mode>      <Joystick>RightStickUp</Joystick>    </Pin>    <Pin>      <Name>Motor Two</Name>      <Number>307</Number>      <Mode>PWMOutput</Mode>      <Joystick>RightStickDown</Joystick>    </Pin>    <Pin>      <Name>Motor +</Name>      <Number>305</Number>      <Mode>Output</Mode>      <Joystick>YBtn</Joystick>    </Pin>    <Pin>      <Name>Motor x</Name>      <Number>304</Number>      <Mode>Output</Mode>      <Joystick>BBtn</Joystick>    </Pin>    <Pin>      <Name>Motor -</Name>      <Number>303</Number>      <Mode>Output</Mode>      <Joystick>ABtn</Joystick>    </Pin>    <Pin>      <Name>Motor Two</Name>      <Number>300</Number>      <Mode>PWMOutput</Mode>      <Joystick>RightTrigger</Joystick>    </Pin>    <Pin>      <Name>NXT +</Name>      <Number>301</Number>      <Mode>Output</Mode>      <Joystick>RightBumper</Joystick>    </Pin>    </PinModes>  <!-- Define HBridges -->  <HBridgeMotors>    <!-- M4 on Motor Hat -->    <HBridgeMotor>      <Name>Motor Two</Name>      <HBridge>607,605,606</HBridge>      <Joystick direction="-1" scale="0.4">RightTrigger</Joystick>    </HBridgeMotor> <!-- M4 on Motor Hat -->    <HBridgeMotor>      <Name>Motor Two</Name>      <HBridge>607,605,606</HBridge>	  <Joystick direction="1" scale="0.4">RightStickUp</Joystick>      <Joystick direction="-1" scale="0.4">RightStickDown</Joystick>    </HBridgeMotor>	  </HBridgeMotors>  <!-- Seven Segment Displays -->  <SevenSegDisplays>    <SevenSegDisplay>      <Name>BoardSevenSeg</Name>      <SegPins>713,715,711,709,708,714,712,710</SegPins>      <Digits>-700,-701,-702</Digits>      <Joystick direction="1">LeftStickUp</Joystick>      <Joystick direction="-1">LeftStickDown</Joystick>    </SevenSegDisplay>  </SevenSegDisplays>  <JoystickFunctions>    <!-- Switch config function -->    <Function>      <Joystick>HomeBtn</Joystick>      <Method assembly="GpioJoy.exe" class="GpioJoy.JoystickManager">ConfigurationPageGoHome</Method>    </Function>    <Function>      <Joystick>BackBtn</Joystick>      <Method assembly="GpioJoy.exe" class="GpioJoy.JoystickManager">ConfigurationPageGoBack</Method>    </Function>    <Function>      <Joystick>StartBtn</Joystick>      <Method assembly="GpioJoy.exe" class="GpioJoy.JoystickManager">ConfigurationPageGoForward</Method>    </Function>	<Assembly>      <Name>GpioDemoBoardUserFunctions.dll</Name>      <Setup class="GpioDemoBoardUserFunctions.UserFunctions">InitXBoxModelPins</Setup>    </Assembly>	    <!-- Motors -->    <Function>      <Joystick>YBtn</Joystick>      <Method assembly="GpioDemoBoardUserFunctions.dll" class="GpioDemoBoardUserFunctions.UserFunctions">YBtnMotorTwoPlus</Method>    </Function>    <Function>      <Joystick>ABtn</Joystick>      <Method assembly="GpioDemoBoardUserFunctions.dll" class="GpioDemoBoardUserFunctions.UserFunctions">ABtnMotorTwoMinus</Method>    </Function>    <Function>      <Joystick>BBtn</Joystick>      <Method assembly="GpioDemoBoardUserFunctions.dll" class="GpioDemoBoardUserFunctions.UserFunctions">BBtnMotorTwoStop</Method>    </Function>    <Function>      <Joystick>DpadUp</Joystick>      <Method assembly="GpioDemoBoardUserFunctions.dll" class="GpioDemoBoardUserFunctions.UserFunctions">DPadUpMotorOnePlus</Method>    </Function>    <Function>      <Joystick>DpadDown</Joystick>      <Method assembly="GpioDemoBoardUserFunctions.dll" class="GpioDemoBoardUserFunctions.UserFunctions">DPadDownMotorOneMinus</Method>    </Function>    <Function>      <Joystick>DpadRight</Joystick>      <Method assembly="GpioDemoBoardUserFunctions.dll" class="GpioDemoBoardUserFunctions.UserFunctions">DPadRightMotorOneStop</Method>    </Function>    <Function>      <Joystick>LeftBumper</Joystick>      <Method assembly="GpioDemoBoardUserFunctions.dll" class="GpioDemoBoardUserFunctions.UserFunctions">LeftBumperMotorNxtMinus</Method>    </Function>    <Function>      <Joystick>RightBumper</Joystick>      <Method assembly="GpioDemoBoardUserFunctions.dll" class="GpioDemoBoardUserFunctions.UserFunctions">RightBumperMotorNxtPlus</Method>    </Function>    <Function>      <Joystick>DpadLeft</Joystick>      <Method assembly="GpioDemoBoardUserFunctions.dll" class="GpioDemoBoardUserFunctions.UserFunctions">DPadLeftMotorNxtStop</Method>    </Function>              </JoystickFunctions></GpioConfig>