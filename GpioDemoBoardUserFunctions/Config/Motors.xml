<?xml version="1.0" encoding="utf-8"?><GpioConfig>    <!--  Configuration for the GPIO Joy Test Board -->    <!-- XBox Joystick connected to MCP expansion chips -->    <!-- Left Side -->    <Pin>      <Name>Motor One</Name>      <Number>207</Number>      <Mode>PWMOutput</Mode>      <Joystick>LeftTrigger</Joystick>    </Pin>    <Pin>      <Name>NXT -</Name>      <Number>206</Number>      <Mode>Output</Mode>      <Joystick>LeftBumper</Joystick>    </Pin>        <Pin>      <Name>Motor One</Name>      <Number>205</Number>      <Mode>PWMOutput</Mode>      <Joystick>LeftStickUp</Joystick>    </Pin>    <Pin>      <Name>Motor One</Name>      <Number>203</Number>      <Mode>PWMOutput</Mode>      <Joystick>LeftStickDown</Joystick>    </Pin>          <Pin>      <Name>Motor +</Name>      <Number>200</Number>      <Mode>Output</Mode>      <Joystick>DpadUp</Joystick>    </Pin>    <Pin>      <Name>Motor x</Name>      <Number>208</Number>      <Mode>Output</Mode>      <Joystick>DpadRight</Joystick>    </Pin>    <Pin>      <Name>Motor -</Name>      <Number>209</Number>      <Mode>Output</Mode>      <Joystick>DpadDown</Joystick>    </Pin>    <Pin>      <Name>NXT x</Name>      <Number>210</Number>      <Mode>Output</Mode>      <Joystick>DpadLeft</Joystick>    </Pin>       <!-- Home Row -->    <Pin>      <Name>Back</Name>      <Number>211</Number>      <Mode>Output</Mode>      <Joystick>BackBtn</Joystick>    </Pin>    <Pin>      <Name>Home</Name>      <Number>212</Number>      <Mode>Output</Mode>      <Joystick>HomeBtn</Joystick>    </Pin>    <Pin>      <Name>Start</Name>      <Number>213</Number>      <Mode>Output</Mode>      <Joystick>StartBtn</Joystick>    </Pin>    <!-- Right Side -->    <Pin>      <Name>Motor Two</Name>      <Number>214</Number>      <Mode>PWMOutput</Mode>      <Joystick>RightStickUp</Joystick>    </Pin>       <Pin>      <Name>Motor Two</Name>      <Number>307</Number>      <Mode>PWMOutput</Mode>      <Joystick>RightStickDown</Joystick>    </Pin>       <Pin>      <Name>Motor +</Name>      <Number>305</Number>      <Mode>Output</Mode>      <Joystick>YBtn</Joystick>    </Pin>    <Pin>      <Name>Motor x</Name>      <Number>304</Number>      <Mode>Output</Mode>      <Joystick>BBtn</Joystick>    </Pin>    <Pin>      <Name>Motor -</Name>      <Number>303</Number>      <Mode>Output</Mode>      <Joystick>ABtn</Joystick>    </Pin>        <Pin>      <Name>Motor Two</Name>      <Number>300</Number>      <Mode>PWMOutput</Mode>      <Joystick>RightTrigger</Joystick>    </Pin>    <Pin>      <Name>NXT +</Name>      <Number>301</Number>      <Mode>Output</Mode>      <Joystick>RightBumper</Joystick>    </Pin>      <!--  Motors on MCP expansion pins -->    <Pin>      <Name>Motor One</Name>      <Number>405</Number>      <Mode>PWMOutput</Mode>      <Joystick scale="0.8">LeftStickUp</Joystick>    </Pin>    <Pin>      <Name>Motor One</Name>      <Number>405</Number>      <Mode>PWMOutput</Mode>      <Joystick scale="0.8">LeftTrigger</Joystick>    </Pin>  </PinModes>  <!-- Define HBridges -->  <HBridgeMotors>    <!-- M4 on Motor Hat -->    <HBridgeMotor>      <ByName>Motor Two</ByName>      <Joystick direction="1" scale="0.4">RightStickUp</Joystick>      <Joystick direction="-1" scale="0.4">RightStickDown</Joystick>    </HBridgeMotor>    <HBridgeMotor>      <ByName>Motor Two</ByName>      <Joystick direction="-1" scale="0.4">RightTrigger</Joystick>    </HBridgeMotor>  </HBridgeMotors>  <!-- Seven Segment Displays -->  <SevenSegDisplays>    <!-- Schema to define seven segment display         <SevenSegDisplay>      <SegPins>713,715,711,709,708,714,712,710,</SegPins>   //  pins for segments a-g plus dot      <Digits>-700,-701,-702</Digits>                       //  pins for digits, use negative numbers for common annode, positive for common cathode    </SevenSegDisplay>    -->    <SevenSegDisplay>      <ByName>RemoteSevenSeg</ByName>      <Joystick direction="1">RightStickUp</Joystick>      <Joystick direction="-1">RightStickDown</Joystick>    </SevenSegDisplay>    <SevenSegDisplay>      <ByName>BoardSevenSeg</ByName>      <Joystick direction="1">LeftStickUp</Joystick>      <Joystick direction="-1">LeftStickDown</Joystick>    </SevenSegDisplay>  </SevenSegDisplays>  <JoystickFunctions>    <!-- Switch config function -->    <Function>      <Joystick>HomeBtn</Joystick>      <Method assembly="GpioJoy.exe" class="GpioJoy.JoystickManager">ConfigurationPageGoHome</Method>    </Function>    <Function>      <Joystick>BackBtn</Joystick>      <Method assembly="GpioJoy.exe" class="GpioJoy.JoystickManager">ConfigurationPageGoBack</Method>    </Function>    <Function>      <Joystick>StartBtn</Joystick>      <Method assembly="GpioJoy.exe" class="GpioJoy.JoystickManager">ConfigurationPageGoForward</Method>    </Function>    <!-- Motors -->    <Function>      <Joystick>YBtn</Joystick>      <Method assembly="GpioDemoBoardUserFunctions.dll" class="GpioDemoBoardUserFunctions.UserFunctions">YBtnMotorTwoPlus</Method>    </Function>    <Function>      <Joystick>ABtn</Joystick>      <Method assembly="GpioDemoBoardUserFunctions.dll" class="GpioDemoBoardUserFunctions.UserFunctions">ABtnMotorTwoMinus</Method>    </Function>    <Function>      <Joystick>BBtn</Joystick>      <Method assembly="GpioDemoBoardUserFunctions.dll" class="GpioDemoBoardUserFunctions.UserFunctions">BBtnMotorTwoStop</Method>    </Function>    <Function>      <Joystick>DpadUp</Joystick>      <Method assembly="GpioDemoBoardUserFunctions.dll" class="GpioDemoBoardUserFunctions.UserFunctions">DPadUpMotorOnePlus</Method>    </Function>    <Function>      <Joystick>DpadDown</Joystick>      <Method assembly="GpioDemoBoardUserFunctions.dll" class="GpioDemoBoardUserFunctions.UserFunctions">DPadDownMotorOneMinus</Method>    </Function>    <Function>      <Joystick>DpadRight</Joystick>      <Method assembly="GpioDemoBoardUserFunctions.dll" class="GpioDemoBoardUserFunctions.UserFunctions">DPadRightMotorOneStop</Method>    </Function>    <Function>      <Joystick>LeftBumper</Joystick>      <Method assembly="GpioDemoBoardUserFunctions.dll" class="GpioDemoBoardUserFunctions.UserFunctions">LeftBumperMotorNxtMinus</Method>    </Function>    <Function>      <Joystick>RightBumper</Joystick>      <Method assembly="GpioDemoBoardUserFunctions.dll" class="GpioDemoBoardUserFunctions.UserFunctions">RightBumperMotorNxtPlus</Method>    </Function>    <Function>      <Joystick>DpadLeft</Joystick>      <Method assembly="GpioDemoBoardUserFunctions.dll" class="GpioDemoBoardUserFunctions.UserFunctions">DPadLeftMotorNxtStop</Method>    </Function>              </JoystickFunctions></GpioConfig>