<?xml version="1.0" encoding="utf-8"?><GpioConfig>  <MCPs>    <MCP>      <Type>Mcp23017</Type>      <!-- chip at 0x23-->      <Address>35</Address>      <Base>200</Base>    </MCP>    <MCP>      <Type>Mcp23008</Type>      <!-- chip at 0x24-->      <Address>36</Address>      <Base>300</Base>    </MCP>    <MCP>      <Type>Mcp23017</Type>      <!-- chip at 0x22-->      <Address>34</Address>      <Base>700</Base>    </MCP>  </MCPs>  <PCAs>    <PCA>      <!-- adafruit pwm hat at default address 0x40-->      <Address>64</Address>      <Base>500</Base>      <Frequency>50</Frequency>    </PCA>  </PCAs>  <PinModes>    <!--  RBG LED -->    <Pin>      <!-- Red -->      <DisplayName>Red</DisplayName>      <Number>513</Number>      <Mode>PWMOutput</Mode>      <Joystick reverse="true">LeftTrigger</Joystick>    </Pin>    <Pin>      <!-- Green -->      <DisplayName>Green</DisplayName>      <Number>514</Number>      <Mode>PWMOutput</Mode>      <Joystick reverse="true">LeftStickUp</Joystick>    </Pin>    <Pin>      <!-- Blue -->      <DisplayName>Blue</DisplayName>      <Number>515</Number>      <Mode>PWMOutput</Mode>      <Joystick reverse="true">RightStickUp</Joystick>    </Pin>    <!-- XBox Joystick Model -->    <!-- Left Side -->    <Pin>      <DisplayName>Red</DisplayName>      <Number>207</Number>      <Mode>PWMOutput</Mode>      <Joystick>LeftTrigger</Joystick>    </Pin>    <Pin>      <DisplayName>Green</DisplayName>      <Number>205</Number>      <Mode>PWMOutput</Mode>      <Joystick>LeftStickUp</Joystick>    </Pin>    <Pin>      <DisplayName>Reset</DisplayName>      <Number>208</Number>      <Mode>Output</Mode>      <Joystick>DpadRight</Joystick>    </Pin>    <!-- Home Row -->    <Pin>      <DisplayName>Back</DisplayName>      <Number>211</Number>      <Mode>Output</Mode>      <Joystick>BackBtn</Joystick>    </Pin>    <Pin>      <DisplayName>Home</DisplayName>      <Number>212</Number>      <Mode>Output</Mode>      <Joystick>HomeBtn</Joystick>    </Pin>    <Pin>      <DisplayName>Next</DisplayName>      <Number>213</Number>      <Mode>Output</Mode>      <Joystick>StartBtn</Joystick>    </Pin>    <!-- Right Side -->    <Pin>      <DisplayName>Blue</DisplayName>      <Number>214</Number>      <Mode>PWMOutput</Mode>      <Joystick>RightStickUp</Joystick>    </Pin>    <Pin>      <DisplayName>Yellow</DisplayName>      <Number>305</Number>      <Mode>Output</Mode>      <Joystick>YBtn</Joystick>    </Pin>    <Pin>      <DisplayName>Red</DisplayName>      <Number>304</Number>      <Mode>Output</Mode>      <Joystick>BBtn</Joystick>    </Pin>    <Pin>      <DisplayName>Green</DisplayName>      <Number>303</Number>      <Mode>Output</Mode>      <Joystick>ABtn</Joystick>    </Pin>    <Pin>      <DisplayName>Blue</DisplayName>      <Number>302</Number>      <Mode>Output</Mode>      <Joystick>XBtn</Joystick>    </Pin>  </PinModes>  <JoystickFunctions>    <Function>      <Joystick>HomeBtn</Joystick>      <Method assembly="GpioJoy.exe" class="GpioJoy.JoystickManager">ConfigurationPageGoHome</Method>    </Function>    <Function>      <Joystick>BackBtn</Joystick>      <Method assembly="GpioJoy.exe" class="GpioJoy.JoystickManager">ConfigurationPageGoBack</Method>    </Function>    <Function>      <Joystick>StartBtn</Joystick>      <Method assembly="GpioJoy.exe" class="GpioJoy.JoystickManager">ConfigurationPageGoForward</Method>    </Function>    <Assembly>      <Name>GpioDemoBoardUserFunctions.dll</Name>      <Setup class="GpioDemoBoardUserFunctions.UserFunctions">InitRgbConfig</Setup>    </Assembly>    <Function>      <Joystick>YBtn</Joystick>      <Method assembly="GpioDemoBoardUserFunctions.dll" class="GpioDemoBoardUserFunctions.UserFunctions">YBtnRGBYellow</Method>    </Function>    <Function>      <Joystick>ABtn</Joystick>      <Method assembly="GpioDemoBoardUserFunctions.dll" class="GpioDemoBoardUserFunctions.UserFunctions">ABtnRGBGreen</Method>    </Function>    <Function>      <Joystick>BBtn</Joystick>      <Method assembly="GpioDemoBoardUserFunctions.dll" class="GpioDemoBoardUserFunctions.UserFunctions">BBtnRGBRed</Method>    </Function>    <Function>      <Joystick>XBtn</Joystick>      <Method assembly="GpioDemoBoardUserFunctions.dll" class="GpioDemoBoardUserFunctions.UserFunctions">XBtnRGBBlue</Method>    </Function>    <Function>      <Joystick>DpadRight</Joystick>      <Method assembly="GpioDemoBoardUserFunctions.dll" class="GpioDemoBoardUserFunctions.UserFunctions">DpadRightRGBReset</Method>    </Function>  </JoystickFunctions></GpioConfig>