<?xml version="1.0" encoding="utf-8"?><GpioConfig>  <!-- GpioConfig  -->  <!-- configuration file for GpioJoy control setup    - Setup instructions    1) Define i2c expander chips  2) Define PCA expander chips  3) Define control pins   4) Define stepper motor drivers    -->  <!-- Enumerations -->  <!--  Pin Mode  public enum GPIOpinmode    {      Input = 0,      Output = 1,      PWMOutput = 2,      GPIOClock = 3    }    -->  <!--  Joystick inputs  public enum JoystickInputItems    {        //  Left Stick        LeftStickUp,        LeftStickDown,        LeftStickRight,        LeftStickLeft,        //  button        LeftStickBtn,        //  Right Stick        RightStickUp,        RightStickDown,        RightStickRight,        RightStickLeft,        //  button        RightStickBtn,        //  Triggers        LeftTrigger,        RightTrigger,        // Buttons        ABtn,        BBtn,        XBtn,        YBtn,        LeftBumper,        RightBumper,        BackBtn,        StartBtn,        HomeBtn,        //  DPad        DpadUp,        DpadDown,        DpadLeft,        DpadRight,    };    -->  <!-- MCP Chip Type    public enum McpType      {        Mcp23017 = 0,        Mcp23008 = 1,      }      -->  <!--  Configuration Step 1:  Define MCP Expander Chips-->  <MCPs>    <!--<MCP>      <Type>Mcp230xx</Type>   //  enum McpType      <Address>32</Address>   //  address should be in decimal      <Base>100</Base>        //  pin base    </MCP>-->    <!--  MCP Chips on the GpioJoy board-->    <MCP>      <Type>Mcp23017</Type>      <!-- chip at 0x23-->      <Address>35</Address>      <Base>200</Base>    </MCP>    <MCP>      <Type>Mcp23008</Type>      <!-- chip at 0x24-->      <Address>36</Address>      <Base>300</Base>    </MCP>    <MCP>      <Type>Mcp23008</Type>      <!--  chip at 0x27 -->      <Address>39</Address>      <Base>400</Base>    </MCP>    <MCP>      <Type>Mcp23017</Type>      <!-- chip at 0x22-->      <Address>34</Address>      <Base>700</Base>    </MCP>        <!-- MCP chips on other boards  -->    <!--<MCP>      <Type>Mcp23017</Type>      --><!-- chip at 0x25-->        <!--      <Address>37</Address>      <Base>800</Base>    </MCP>-->      </MCPs>  <!--  Configuration Step 2:  Define PCA Expander Chips-->  <PCAs>    <!--<PCA>      <Address>35</Address>       //  address should be in decimal      <Base>300</Base>            //  pin base      <Frequency>50</Frequency>   //  frequency in hertz    </PCA>-->    <PCA>      <!-- adafruit pwm hat at default address 0x40-->      <Address>64</Address>      <Base>500</Base>      <Frequency>50</Frequency>    </PCA>    <PCA>      <!-- adafruit motor hat at default address 0x60-->      <Address>96</Address>      <Base>600</Base>      <Frequency>50</Frequency>    </PCA>  </PCAs>  <!-- Configuration Step 3:  Define Control Pins -->  <ControlPins>    <!--  Schema for defining control pins        //  Init output for pin number N    <Pin>      <Number>N</Number>      <Mode>Output</Mode>    </Pin>        //  Init output for pwm pin number N     <Pin>      <Number>N</Number>      <Mode>PWMOutput</Mode>      <Range>100</Range>        //  range is optional, default 100 for software PWM, hard coded 1024 for RPi pin 12 and 4096 for PCA pins     </Pin>        //  Init output for pwm pin number N with joystick     <Pin>      <Number>N</Number>      <Mode>PWMOutput</Mode>      <Joystick>LeftTrigger</Joystick>     </Pin>        //  Init output for pwm pin number N with hbridge (joystick assignment is not required)     <Pin>      <Number>N</Number>      <Mode>PWMOutput</Mode>      <Joystick direction="1">LeftStickUp</Joystick>      <Joystick direction="-1">LeftStickDown</Joystick>     </Pin>    -->    <!--  Raspberry Pi Pins -->    <Pin>      <Number>7</Number>      <Mode>Output</Mode>    </Pin>    <Pin>      <Number>11</Number>      <Mode>Output</Mode>    </Pin>    <Pin>      <Number>12</Number>      <Mode>PWMOutput</Mode>    </Pin>    <Pin>      <Number>13</Number>      <Mode>Output</Mode>    </Pin>    <Pin>      <Number>15</Number>      <Mode>Output</Mode>    </Pin>    <Pin>      <Number>16</Number>      <Mode>Output</Mode>    </Pin>    <Pin>      <Number>18</Number>      <Mode>Output</Mode>    </Pin>    <Pin>      <Number>22</Number>      <Mode>Output</Mode>    </Pin>    <Pin>      <Number>29</Number>      <Mode>Output</Mode>    </Pin>    <Pin>      <Number>31</Number>      <Mode>Output</Mode>    </Pin>    <Pin>      <Number>32</Number>      <Mode>Output</Mode>    </Pin>    <Pin>      <Number>33</Number>      <Mode>Output</Mode>    </Pin>    <Pin>      <Number>35</Number>      <Mode>Output</Mode>    </Pin>    <Pin>      <Number>36</Number>      <Mode>Output</Mode>    </Pin>    <Pin>      <Number>37</Number>      <Mode>Output</Mode>    </Pin>    <Pin>      <Number>38</Number>      <Mode>Output</Mode>    </Pin>    <Pin>      <Number>40</Number>      <Mode>Output</Mode>    </Pin>    <!--  Configuration for the GPIO Joy Test Board -->    <!-- XBox Joystick connected to MCP expansion chips -->    <!-- Left Side -->    <Pin>      <Name>Motor One</Name>      <Number>207</Number>      <Mode>PWMOutput</Mode>      <Joystick>LeftTrigger</Joystick>    </Pin>    <Pin>      <Name>Start Demo</Name>      <Number>206</Number>      <Mode>Output</Mode>      <Joystick>LeftBumper</Joystick>    </Pin>    <Pin>      <Name>Servo One</Name>      <Number>205</Number>      <Mode>PWMOutput</Mode>      <Joystick>LeftStickUp</Joystick>    </Pin>    <Pin>      <Name> </Name>      <Number>204</Number>      <Mode>PWMOutput</Mode>      <Joystick>LeftStickRight</Joystick>    </Pin>    <Pin>      <Name>Servo One</Name>      <Number>203</Number>      <Mode>PWMOutput</Mode>      <Joystick>LeftStickDown</Joystick>    </Pin>    <Pin>      <Name> </Name>      <Number>202</Number>      <Mode>PWMOutput</Mode>      <Joystick>LeftStickLeft</Joystick>    </Pin>    <Pin>      <Name> </Name>      <Number>201</Number>      <Mode>Output</Mode>      <Joystick>LeftStickBtn</Joystick>    </Pin>    <Pin>      <Name>Servo +</Name>      <Number>200</Number>      <Mode>Output</Mode>      <Joystick>DpadUp</Joystick>    </Pin>    <Pin>      <Name>Servo Center</Name>      <Number>208</Number>      <Mode>Output</Mode>      <Joystick>DpadRight</Joystick>    </Pin>    <Pin>      <Name>Servo -</Name>      <Number>209</Number>      <Mode>Output</Mode>      <Joystick>DpadDown</Joystick>    </Pin>    <Pin>      <Name> </Name>      <Number>210</Number>      <Mode>Output</Mode>      <Joystick>DpadLeft</Joystick>    </Pin>    <!-- Home Row -->    <Pin>      <Name>Back</Name>      <Number>211</Number>      <Mode>Output</Mode>      <Joystick>BackBtn</Joystick>    </Pin>    <Pin>      <Name>Home</Name>      <Number>212</Number>      <Mode>Output</Mode>      <Joystick>HomeBtn</Joystick>    </Pin>    <Pin>      <Name>Start</Name>      <Number>213</Number>      <Mode>Output</Mode>      <Joystick>StartBtn</Joystick>    </Pin>    <!-- Right Side -->    <Pin>      <Name>Motor Two</Name>      <Number>214</Number>      <Mode>PWMOutput</Mode>      <Joystick>RightStickUp</Joystick>    </Pin>    <Pin>      <Name> </Name>      <Number>215</Number>      <Mode>PWMOutput</Mode>      <Joystick>RightStickRight</Joystick>    </Pin>    <Pin>      <Name>Motor Two</Name>      <Number>307</Number>      <Mode>PWMOutput</Mode>      <Joystick>RightStickDown</Joystick>    </Pin>    <Pin>      <Name> </Name>      <Number>306</Number>      <Mode>PWMOutput</Mode>      <Joystick>RightStickLeft</Joystick>    </Pin>    <Pin>      <Name> </Name>      <Number>407</Number>      <Mode>Output</Mode>      <Joystick>RightStickBtn</Joystick>    </Pin>    <Pin>      <Name>Motor +</Name>      <Number>305</Number>      <Mode>Output</Mode>      <Joystick>YBtn</Joystick>    </Pin>    <Pin>      <Name>Motor x</Name>      <Number>304</Number>      <Mode>Output</Mode>      <Joystick>BBtn</Joystick>    </Pin>    <Pin>      <Name>Motor -</Name>      <Number>303</Number>      <Mode>Output</Mode>      <Joystick>ABtn</Joystick>    </Pin>    <Pin>      <Name> </Name>      <Number>302</Number>      <Mode>Output</Mode>      <Joystick>XBtn</Joystick>    </Pin>    <Pin>      <Name>Motor One</Name>      <Number>300</Number>      <Mode>PWMOutput</Mode>      <Joystick>RightTrigger</Joystick>    </Pin>    <Pin>      <Name>Stop Demo</Name>      <Number>301</Number>      <Mode>Output</Mode>      <Joystick>RightBumper</Joystick>    </Pin>    <!--  Motors on MCP expansion pins -->    <Pin>      <Name>Motor One</Name>      <Number>405</Number>      <Mode>PWMOutput</Mode>      <Joystick scale="0.8">LeftTrigger</Joystick>    </Pin>    <!--  Stepper motor on MCP -->    <Pin>      <Number>401</Number>      <Mode>Output</Mode>    </Pin>    <Pin>      <Number>402</Number>      <Mode>Output</Mode>    </Pin>    <Pin>      <Number>403</Number>      <Mode>Output</Mode>    </Pin>    <Pin>      <Number>404</Number>      <Mode>Output</Mode>    </Pin>    <!--  Seven segment display -->    <!-- Digits -->    <Pin>      <!-- White -->      <Number>700</Number>      <Mode>Output</Mode>    </Pin>    <Pin>      <!-- Grey -->      <Number>701</Number>      <Mode>Output</Mode>    </Pin>    <Pin>      <!-- Black  -->      <Number>702</Number>      <Mode>Output</Mode>    </Pin>    <!-- Segments a - g and decimal -->    <Pin>      <!-- a Purple/Silver -->      <Number>713</Number>      <Mode>Output</Mode>    </Pin>    <Pin>      <!-- b Yellow/Silver -->      <Number>715</Number>      <Mode>Output</Mode>    </Pin>    <Pin>      <!-- c Orange -->      <Number>711</Number>      <Mode>Output</Mode>    </Pin>    <Pin>      <!-- d Blue -->      <Number>709</Number>      <Mode>Output</Mode>    </Pin>    <Pin>      <!-- e Purple -->      <Number>708</Number>      <Mode>Output</Mode>    </Pin>    <Pin>      <!-- f Black/Silver-->      <Number>714</Number>      <Mode>Output</Mode>    </Pin>    <Pin>      <!-- g Brown -->      <Number>712</Number>      <Mode>Output</Mode>    </Pin>    <Pin>      <!-- dot Yellow -->      <Number>710</Number>      <Mode>Output</Mode>    </Pin>    <!--  Seven segment display on proto board -->    <!-- Digits --><!--    <Pin>      <Number>800</Number>      <Mode>Output</Mode>    </Pin>    <Pin>      <Number>801</Number>      <Mode>Output</Mode>    </Pin>    <Pin>      <Number>802</Number>      <Mode>Output</Mode>    </Pin>    --><!-- Segments a - g and decimal --><!--    <Pin>      <Number>813</Number>      <Mode>Output</Mode>    </Pin>    <Pin>      <Number>815</Number>      <Mode>Output</Mode>    </Pin>    <Pin>      <Number>811</Number>      <Mode>Output</Mode>    </Pin>    <Pin>      <Number>809</Number>      <Mode>Output</Mode>    </Pin>    <Pin>      <Number>808</Number>      <Mode>Output</Mode>    </Pin>    <Pin>      <Number>814</Number>      <Mode>Output</Mode>    </Pin>    <Pin>      <Number>812</Number>      <Mode>Output</Mode>    </Pin>    <Pin>      <Number>810</Number>      <Mode>Output</Mode>    </Pin>-->            <!-- Unused pins on MCP -->    <Pin>      <Number>400</Number>      <Mode>Output</Mode>    </Pin>    <Pin>      <Number>406</Number>      <Mode>Output</Mode>    </Pin>    <Pin>      <Number>703</Number>      <Mode>Output</Mode>    </Pin>    <Pin>      <Number>704</Number>      <Mode>Output</Mode>    </Pin>    <Pin>      <Number>705</Number>      <Mode>Output</Mode>    </Pin>    <Pin>      <Number>706</Number>      <Mode>Output</Mode>    </Pin>    <Pin>      <Number>707</Number>      <Mode>Output</Mode>    </Pin>    <!--<Pin>      <Number>803</Number>      <Mode>Output</Mode>    </Pin>    <Pin>      <Number>804</Number>      <Mode>Output</Mode>    </Pin>    <Pin>      <Number>805</Number>      <Mode>Output</Mode>    </Pin>    <Pin>      <Number>806</Number>      <Mode>Output</Mode>    </Pin>    <Pin>      <Number>807</Number>      <Mode>Output</Mode>    </Pin>-->    <!--  Adafruit PWM driver-->    <Pin>      <Name>Servo One</Name>      <Number>500</Number>      <Mode>PWMOutput</Mode>      <Joystick servo="true" direction="1">LeftStickUp</Joystick>      <Joystick servo="true" direction="-1">LeftStickDown</Joystick>    </Pin>    <Pin>      <Name>Servo Two</Name>      <Number>501</Number>      <Mode>PWMOutput</Mode>    </Pin>    <Pin>      <Number>502</Number>      <Mode>PWMOutput</Mode>    </Pin>    <Pin>      <Number>503</Number>      <Mode>PWMOutput</Mode>    </Pin>    <Pin>      <Number>504</Number>      <Mode>PWMOutput</Mode>    </Pin>    <Pin>      <Number>505</Number>      <Mode>PWMOutput</Mode>    </Pin>    <Pin>      <Number>506</Number>      <Mode>PWMOutput</Mode>    </Pin>    <Pin>      <Number>507</Number>      <Mode>PWMOutput</Mode>    </Pin>    <Pin>      <Number>508</Number>      <Mode>PWMOutput</Mode>    </Pin>    <Pin>      <Number>509</Number>      <Mode>PWMOutput</Mode>    </Pin>    <Pin>      <Number>510</Number>      <Mode>PWMOutput</Mode>    </Pin>    <Pin>      <Number>511</Number>      <Mode>PWMOutput</Mode>    </Pin>    <Pin>      <Number>512</Number>      <Mode>PWMOutput</Mode>    </Pin>    <Pin>      <Number>513</Number>      <Mode>PWMOutput</Mode>    </Pin>    <Pin>      <Number>514</Number>      <Mode>PWMOutput</Mode>    </Pin>    <Pin>      <Number>515</Number>      <Mode>PWMOutput</Mode>    </Pin>    <!--  Adafruit motor hat-->    <Pin>      <Number>602</Number>      <Mode>PWMOutput</Mode>    </Pin>    <Pin>      <Number>603</Number>      <Mode>Output</Mode>    </Pin>    <Pin>      <Number>604</Number>      <Mode>Output</Mode>    </Pin>    <Pin>      <Number>605</Number>      <Mode>Output</Mode>    </Pin>    <Pin>      <Number>606</Number>      <Mode>Output</Mode>    </Pin>    <Pin>      <Number>607</Number>      <Mode>PWMOutput</Mode>    </Pin>    <Pin>      <Number>608</Number>      <Mode>PWMOutput</Mode>    </Pin>    <Pin>      <Number>609</Number>      <Mode>Output</Mode>    </Pin>    <Pin>      <Number>610</Number>      <Mode>Output</Mode>    </Pin>    <Pin>      <Number>611</Number>      <Mode>Output</Mode>    </Pin>    <Pin>      <Number>612</Number>      <Mode>Output</Mode>    </Pin>    <Pin>      <Number>613</Number>      <Mode>PWMOutput</Mode>    </Pin>  </ControlPins>  <!--  Define Stepper Motor Drivers  -->  <StepperDrivers>    <!--  Schema for defining stepper motor	    //  Stepper sequence for single steps     <StepperSequence>      <Id>1</Id>      <SequenceItems>        <Item>1, 0, 0, 0</Item>        <Item>0, 1, 0, 0</Item>        <Item>0, 0, 1, 0</Item>        <Item>0, 0, 0, 1 </Item>      </SequenceItems>    </StepperSequence>        //  Stepper Sequence for single steps, more torque    <StepperSequence>     <Id>2</Id>		 <SequenceItems>		  <Item>1,1,0,0</Item>		  <Item>0,1,1,0</Item>		  <Item>0,0,1,1</Item>		  <Item>1,0,0,1</Item>		 </SequenceItems>	  </StepperSequence>            //  Stepper sequence for half steps, even more torque but turns slower    <StepperSequence>       <Id>2</Id>      <SequenceItems>        <Item>1,0,0,0</Item>        <Item>1,1,0,0</Item>        <Item>0,1,0,0</Item>        <Item>0,1,1,0</Item>        <Item>0,0,1,0</Item>        <Item>0,0,1,1</Item>        <Item>0,0,0,1</Item>        <Item>1,0,0,1</Item>      </SequenceItems>    </StepperSequence>             //  Simple unipolar stepper    <StepperDriver>      <Sequence>1</Sequence>			<Pins>401,402,403,404</Pins>		</StepperDriver>        //Bipolar stepper motor motor    <StepperDriver>      <Sequence>1</Sequence>      <Pins>608,-613,-608, 613</Pins>             <PwmScale>0.3</PwmScale>              //  optional parameter, use this to have PWM current limiting on stepper pins - note works only with PCA chip pins, do not use with software PWM      <Polarity>608,610,609</Polarity>      //  where 610 is high and 609 is low to get voltage positive at 608      <Polarity>613,611,612</Polarity>    </StepperDriver>            -->    <!-- Stepper Sequence -->    <StepperSequence>      <Id>1</Id>      <SequenceItems>        <Item>1, 0, 0, 0</Item>        <Item>0, 1, 0, 0</Item>        <Item>0, 0, 1, 0</Item>        <Item>0, 0, 0, 1 </Item>      </SequenceItems>    </StepperSequence>    <StepperSequence>      <Id>2</Id>      <SequenceItems>        <Item>1, 0, 0, 1</Item>        <Item>1, 1, 0, 0</Item>        <Item>0, 1, 1, 0</Item>        <Item>0, 0, 1, 1 </Item>      </SequenceItems>    </StepperSequence>    <!--Stepper Driver -->    <!--  Stepper driver for unipolar on MCP chip-->    <StepperDriver>      <Name>Stepper One</Name>      <Sequence>1</Sequence>      <Pins>401,402,403,404</Pins>      <MinDelay>2</MinDelay>      <MaxDelay>100</MaxDelay>      <Joystick direction="1">RightStickUp</Joystick>      <Joystick direction="-1">RightStickDown</Joystick>    </StepperDriver>    <!-- Stpeer driver for bipolar on PCA chip  -->    <!-- wired to M1 and M2 terminals on the Adafruit motor hat -->    <StepperDriver>      <Name>Stepper Two</Name>      <Sequence>1</Sequence>      <Pins>608,-613,-608, 613</Pins>      <!--<PwmScale>0.5</PwmScale>-->      <MinDelay>15</MinDelay>      <MaxDelay>500</MaxDelay>      <Polarity>608,610,609</Polarity>      <Polarity>613,611,612</Polarity>    </StepperDriver>  </StepperDrivers>  <!-- Define HBridges -->  <HBridges>    <!-- Schema for defining HBridge         <HBridge>        <Polarity>608,610,609</Polarity>      // where 608 is the pwm pin, 610 is the positive polarity pin and 609 is the negative polarity pin        <Joystick direction="1">RightStickUp</Joystick>    //  Joystick optional        <Joystick direction="-1">RightStickDown</Joystick>     </HBridge>    -->    <!-- M4 on Motor Hat -->    <HBridge>      <Name>Motor Two</Name>      <Polarity>607,605,606</Polarity>      <Joystick direction="-1" scale="0.4">RightTrigger</Joystick>    </HBridge>    <!-- M4 on Motor Hat -->    <HBridge>      <Name>Motor NXT</Name>      <Polarity>12,11,13</Polarity>    </HBridge>  </HBridges>  <!-- Seven Segment Displays -->  <SevenSegDisplays>    <!-- Schema to define seven segment display         <SevenSegDisplay>      <SegPins>713,715,711,709,708,714,712,710,</SegPins>   //  pins for segments a-g plus dot      <Digits>-700,-701,-702</Digits>                       //  pins for digits, use negative numbers for common annode, positive for common cathode    </SevenSegDisplay>    -->    <!--<SevenSegDisplay>      <Name>RemoteSevenSeg</Name>      <SegPins>815,813,809,811,812,814,808,810</SegPins>      <Digits>-800,-801,-802</Digits>      <Joystick direction="1">RightStickUp</Joystick>      <Joystick direction="-1">RightStickDown</Joystick>    </SevenSegDisplay>-->    <SevenSegDisplay>      <Name>BoardSevenSeg</Name>      <SegPins>713,715,711,709,708,714,712,710</SegPins>      <Digits>-700,-701,-702</Digits>      <Joystick direction="1">LeftStickUp</Joystick>      <Joystick direction="-1">LeftStickDown</Joystick>    </SevenSegDisplay>  </SevenSegDisplays>  <JoystickFunctions>    <Assembly>      <Name>GpioDemoBoardUserFunctions.dll</Name>      <Setup class="GpioDemoBoardUserFunctions.UserFunctions">InitXBoxModelPins</Setup>    </Assembly>    <!-- Schema to define joystick inputs mapped to methods -->    <!--     <Function>      <Joystick>HomeBtn</Joystick>      <Method assembly="GpioJoy.exe" class="GpioJoy.JoystickManager">HomeBtn</Method>    //  where first item is class and second item is function name                                                  //  if the joystick is button, this funciton must take single bool argument, if stick this function must take single double argument    </Function>    -->    <!-- Switch config function -->    <Function>      <Joystick>HomeBtn</Joystick>      <Method assembly="GpioJoy.exe" class="GpioJoy.JoystickManager">ConfigurationPageGoHome</Method>    </Function>         <Function>      <Joystick>BackBtn</Joystick>       <Method assembly="GpioJoy.exe" class="GpioJoy.JoystickManager">ConfigurationPageGoBack</Method>    </Function>           <Function>      <Joystick>StartBtn</Joystick>       <Method assembly="GpioJoy.exe" class="GpioJoy.JoystickManager">ConfigurationPageGoForward</Method>    </Function>        <!--Start/Stop Demo-->    <Function>      <Joystick>LeftBumper</Joystick>      <Method assembly="GpioDemoBoardUserFunctions.dll" class="GpioDemoBoardUserFunctions.UserFunctions">LeftBumperStartGpioJoyDemo</Method>    </Function>    <Function>      <Joystick>RightBumper</Joystick>      <Method assembly="GpioDemoBoardUserFunctions.dll" class="GpioDemoBoardUserFunctions.UserFunctions">RightBumperStopGpioJoyDemo</Method>    </Function>    <!--Servo control-->    <Function>      <Joystick>DpadUp</Joystick>      <Method assembly="GpioDemoBoardUserFunctions.dll" class="GpioDemoBoardUserFunctions.UserFunctions">DPadUpServoPlus</Method>    </Function>    <Function>      <Joystick>DpadDown</Joystick>      <Method assembly="GpioDemoBoardUserFunctions.dll" class="GpioDemoBoardUserFunctions.UserFunctions">DPadDownServoMinus</Method>    </Function>    <Function>      <Joystick>DpadRight</Joystick>      <Method assembly="GpioDemoBoardUserFunctions.dll" class="GpioDemoBoardUserFunctions.UserFunctions">DPadRightServoCenter</Method>    </Function>    <!-- Motors -->    <Function>      <Joystick>YBtn</Joystick>      <Method assembly="GpioDemoBoardUserFunctions.dll" class="GpioDemoBoardUserFunctions.UserFunctions">YBtnMotorTwoPlus</Method>    </Function>    <Function>      <Joystick>ABtn</Joystick>      <Method assembly="GpioDemoBoardUserFunctions.dll" class="GpioDemoBoardUserFunctions.UserFunctions">ABtnMotorTwoMinus</Method>    </Function>    <Function>      <Joystick>BBtn</Joystick>      <Method assembly="GpioDemoBoardUserFunctions.dll" class="GpioDemoBoardUserFunctions.UserFunctions">BBtnMotorTwoStop</Method>    </Function>  </JoystickFunctions></GpioConfig>